using System;

class Solution
{
    static void Main(String[] args)
    {
        int n = Int32.Parse(Console.ReadLine());
        int[] arr = Array.ConvertAll(Console.ReadLine().Split(' '), int.Parse);

        int sum = 0;

        for (int i = 0; i < n; i++)
        {
            sum += arr[i];
        }

        Console.WriteLine(sum);
    }
}
